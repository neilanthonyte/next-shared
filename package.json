{
  "name": "next-shared",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf ./dist tsconfig.tsbuildinfo",
    "test": "jest",
    "testpipeline": "jest --no-cache --ci",
    "reset": "yarn clean && yarn build",
    "prettier": "npx prettier --check \"src/**/*.{ts,tsx,js,jsx,scss}\"",
    "prettier:fix": "npx prettier --write \"src/**/*.{ts,tsx,js,jsx,scss}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "fix": "yarn prettier:fix && yarn lint:fix",
    "validate": "yarn prettier && yarn lint"
  },
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.3.1",
    "libphonenumber-js": "^1.9.50",
    "typescript": "^4.3.5",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {}
}
